{
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg":"iAgent"
  },
  "subscribe": {
    "config": "ALM.RALLY.config"
  },
  "publish": {
    "data": "ALM.RALLY.DATA",
