{
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg":"iAgent"
  },
  "subscribe": {
    "config": "ALM.JIRA.config"
  },
  "publish": {
    "data": "ALM.JIRA.DATA",
    "health": "ALM.JIRA.HEALTH"
  },
  "communication": {
    "type": "REST",
    "sslVerify": true,
    "responseType": "JSON"
  },
 "dynamicTemplate": {
 	   "timeFieldMapping" : {
        "creationDate" : "%Y-%m-%dT%H:%M:%S",
		"startDate" : "%Y-%m-%d"
  		}, 		
	  "responseTemplate": {
	    "key": "key",
	    "fields": {
	      "issuetype": {
	        "name": "issueType"
	      },
		  "created": "creationDate",
	      "updated": "lastUpdated",
	      "project": {
	        "name": "projectName"
	      },
	      "creator": {
	        "name": "creator"
	      },
	      "priority": {
	        "name": "priority"
	      },
	      "assignee": {
	        "name": "assignee"
	      },
	      "status": {
	        "name": "status"
	      },      
	      "issuelinks": [
	        {
	          "outwardIssue": {
	            "key": "outwardIssue"
	          }
	        },
	        {
	          "inwardIssue": {
	            "key": "inwardIssue"
	          }
	        }
	      ]
	    } 
	  },
	  "extensions" : {
        "sprints" : {
                "runSchedule" : 720,
                "boardApiUrl" : "#JIRA_URL#/rest/agile/1.0/board",
                "sprintApiUrl" : "#JIRA_URL#/rest/agile/1.0/sprint",
                "sprintResponseTemplate" : {
                        "id": "sprintId",
                        "state": "state",
                        "name": "sprintName",
                        "startDate": "sprintStartDate",
                        "endDate": "sprintEndDate",
                        "completeDate": "sprintCompleteDate",
                        "originBoardId": "boardId"
                },
                "sprintMetadata" : {
                "labels" : ["SPRINT"],
                "dataUpdateSupported" : true,
                "uniqueKey" : ["boardId", "sprintId"]
            }
        },
		  	"sprintReport" : {
		  		"runSchedule" : 720,
		  		"boardApiUrl" : "https://<JIRA_HOST>/rest/agile/1.0/board",
			  	"sprintReportUrl" : "https://<JIRA_HOST>/rest/greenhopper/1.0/rapid/charts/sprintreport",
			  	"sprintReportResponseTemplate" : {
			        "key": "key",
			        "typeName": "issueType",
			        "priorityName": "priority",
			        "currentEstimateStatistic": {
			          "statFieldValue": {
			            "value": "currentEstimate"
			          }
			        },
			        "estimateStatistic": {
			          "statFieldValue": {
			            "value": "committedEstimate"
			          }
			        },
			    	"statusName": "status"
			    },
			    "relationMetadata" : {
			        "labels" : ["LATEST"],
			        "relation" : {
			                "properties" : ["addedDuringSprint", "sprintIssueRegion", "committedEstimate"],
			                "name" : "SPRINT_HAS_ISSUES",
			                "source" : {
			                        "constraints" : ["sprintId", "boardId"]
			                    },
			                "destination" : {
			                        "constraints" : ["key"]
			                    }
			            }
			    },
			    "sprintMetadata" : {
			        "labels" : ["SPRINT"],
			        "dataUpdateSupported" : true,
			        "uniqueKey" : ["boardId", "sprintId"]
			    }
		  	},
		  	"releaseDetails" : {
		  		"runSchedule" : 720,
			  	"jiraProjectApiUrl" : "https://<JIRA_HOST>/rest/api/2/project",
			  	"jiraProjectResponseTemplate" : {
				    "id": "projectId",
				    "key": "projectKey",
				    "name": "projectName",
				    "projectCategory": {
				      "name": "projectCategory"
				    },
				    "projectTypeKey": "projectType"
				},
				"jiraReleaseResponseTemplate" : {
				    "id": "versionId",
				    "name": "versionName",
				    "archived": "archived",
				    "released": "released",
				    "releaseDate": "releaseDate",
				    "projectId": "projectId"
				},
				"releaseVersionsMetadata" : {
			        "labels" : ["RELEASE"],
			        "dataUpdateSupported" : true,
			        "uniqueKey" : ["projectKey", "projectId"]
			    }
			 }
 		}
  },
  "toolCategory" : "ALM",
  "enableSprintReport" : false,
  "enableDataValidation": true,
  "sprintField": "sprintField",
  "enableValueArray": true, 
  "isDebugAllowed" : false,  
  "runSchedule": 30,
  "sprintRetrivalSchedule" : 360,
  "dataFetchCount": 1000,
  "userid": "userid",
  "passwd": "password",
  "baseUrl": "https://127.0.0.1/rest/api/2/search",
  "startFrom": "2016-10-10 00:01",
  "toolsTimeZone" : "Asia/Kolkata",
  "insightsTimeZone" : "Asia/Kolkata",
  "useResponseTemplate" : true,
  "timeStampField":"lastUpdated",
  "timeStampFormat":"%Y-%m-%dT%H:%M:%S",
  "isEpochTimeFormat" : false,
  "loggingSetting" : {
		"logLevel" : "WARN",
		"maxBytes" : 5000000,
		"backupCount" : 1000
	}
}