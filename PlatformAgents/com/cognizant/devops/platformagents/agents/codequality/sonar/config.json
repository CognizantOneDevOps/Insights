{
  "category": "CODEQUALITY",
  "communication": {
    "type": "REST",
    "sslVerify": true,
    "responseType": "JSON"
  },
  "mqConfig": {
    "exchange": "iSight",
    "host": "127.0.0.1",
    "password": "iSight",
    "user": "iSight",
	"agentControlXchg":"iAgent"
  },
  "publish": {
    "data": "CODEQUALITY.SONAR.DATA",
    "health": "CODEQUALITY.SONAR.HEALTH"
  },
  "subscribe": {
    "config": "CODEQUALITY.SONAR.config"
  },
  "toolCategory" : "CODEQUALITY",
  "dynamicTemplate" : {
  		"timeFieldMapping" : {
			"startDate" : "%Y-%m-%d"
  		},
  		"metrics" : ["alert_status", "complexity", "coverage", "new_coverage"]
  },
  "toolName": "SONAR",
  "runSchedule": 30,
  "enableDataValidation": true,
  "timeStampField": "metricdate",
  "userId":"admin",
  "password":"admin",
  "timeMachineapi": "no",
  "timeStampFormat": "%Y-%m-%dT%H:%M:%S",
  "startFrom": "2015-10-10 00:01:01",
  "toolsTimeZone": "Asia/Kolkata",
  "useResponseTemplate": false,
  "isDebugAllowed": false,
  "enableValueArray": false,
  "isEpochTimeFormat" : false,
  "baseUrl": "http://127.0.0.1:9000/sonar/",
  "loggingSetting": {
    	"logLevel": "WARN",
		"maxBytes" : 5000000,
		"backupCount" : 1000
  }
}
