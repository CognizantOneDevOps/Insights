{
  "vault": {
    "getFromVault": false,
    "secretEngine": "insights-kv",
    "readToken": "vault_secret_token",
    "vaultUrl": "http://Vault_Host:port/v1/"
  },
  "mqConfig": {
    "user": "username for RabbitMq",
    "password": "",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg": "iAgent",
    "port": 5672,
    "enableDeadLetterExchange": false,
    "prefetchCount": 5
  },
  "subscribe": {
    "config": "SCM.GIT.config"
  },
  "publish": {
    "data": "SCM.GIT.DATA",
    "health": "SCM.GIT.HEALTH"
  },
  "communication": {
    "type": "REST",
    "sslVerify": false,
    "responseType": "JSON"
  },
  "dynamicTemplate": {
    "timeFieldMapping": {
      "startDate": "%Y-%m-%d",
      "createdAt": "%Y-%m-%dT%H:%M:%SZ",
      "updatedAt": "%Y-%m-%dT%H:%M:%SZ",
      "closedAt": "%Y-%m-%dT%H:%M:%SZ",
      "mergedAt": "%Y-%m-%dT%H:%M:%SZ",
      "commitTime": "%Y-%m-%dT%H:%M:%SZ",
      "since": "%Y-%m-%dT%H:%M:%SZ",
      "until": "%Y-%m-%dT%H:%M:%SZ",
      "consumptionTime": "%Y-%m-%dT%H:%M:%SZ"
    },
    "repositories": {
    	"names": []
    },
    "commit": {
      "commitResponseTemplate": {
        "sha": "commitId",
        "commit": {
          "message": "message",
          "author": {
            "name": "authorName",
            "date": "commitTime"
          }
        }
      },
      "commitMetadata": {
        "dataUpdateSupported": true,
        "uniqueKey": [
          "repoName",
          "commitId",
          "gitType"
        ]
      }
    },
    "pullRequest": {
      "insightsTimeXFieldMapping": {
        "timefield": "updatedAt",
        "isEpoch": false,
        "timeformat": "%Y-%m-%dT%H:%M:%SZ"
      },
      "pullReqResponseTemplate": {
        "number": "pullReqId",
        "state": "pullReqState",
        "head": {
          "sha": "headSHA",
          "ref": "originBranch",
          "repo": {
            "fork": "isForked"
          }
        },
        "base": {
          "sha": "baseSHA",
          "ref": "baseBranch"
        },
        "isMerged": "isMerged",
        "originBranchAlmKeys": "originBranchAlmKeys",
        "merge_commit_sha": "mergedSHA",
        "commit": "commit",
        "created_at": "createdAt",
        "updated_at": "updatedAt",
        "closed_at": "closedAt",
        "merged_at": "mergedAt"
      },
      "metaData": {
        "dataUpdateSupported": true,
        "uniqueKey": [
          "repoName",
          "pullReqId",
          "gitType"
        ]
      }
    },
    "branch": {
      "insightsTimeXFieldMapping": {
        "timefield": "consumptionTime",
        "isEpoch": false,
        "timeformat": "%Y-%m-%dT%H:%M:%SZ"
      },
      "branchMetadata": {
        "labels": ["GIT_BRANCH"],
		"dataUpdateSupported": true,
		"uniqueKey": [
			"repoName",
			"branchName"
		]
      }
    },
    "extensions": {
      "commitFileDetails": {
        "insightsTimeXFieldMapping": {
          "timefield": "commitTime",
          "isEpoch": false,
          "timeformat": "%Y-%m-%dT%H:%M:%SZ"
        },
        "runSchedule": 60,
        "relationMetadata": {
          "relation": {
            "properties": [
              "commitTime",
              "authorName",
              "additions",
              "deletions",
              "changes",
              "status"
            ],
            "name": "FILE_HAS_COMMITS",
            "source": {
              "labels": [
                "LATEST"
              ],
              "constraints": [
                "commitId"
              ]
            },
            "destination": {
              "labels": [
                "FILE_CHANGE"
              ],
              "constraints": [
                "filepathHash",
                "filename"
              ],
              "selectedProperties": [
                "inSightsTimeX",
                "inSightsTime",
                "toolName",
                "categoryName",
                "fileExtension"
              ]
            }
          }
        }
      },
      "commitBranchRelation": {
        "relationMetadata": {
          "relation": {
            "properties": [
              "commitTime",
              "authorName"
            ],
            "name": "BRANCH_HAS_COMMITS",
            "source": {
              "labels": [
                "LATEST"
              ],
              "constraints": [
                "branchName",
                "repoName",
                "commitId"
              ]
            },
            "destination": {
              "labels": [
                "GIT_BRANCH",
                "LATEST"
              ],
              "constraints": [
                "branchName",
                "repoName"
              ],
              "selectedProperties": [
                "inSightsTimeX",
                "inSightsTime",
                "toolName",
                "categoryName"
              ]
            }
          }
        }
      },
      "PullReqBranchRelation": {
        "relationMetadata": {
          "relation": {
            "properties": [
              "mergedSHA"
            ],
            "name": "BRANCH_HAS_PULL_REQUESTS",
            "source": {
              "labels": [
                "LATEST"
              ],
              "constraints": [
                "originBranch",
                "repoName"
              ]
            },
            "destination": {
              "labels": [
                "GIT_BRANCH",
                "LATEST"
              ],
              "constraints": [
                "branchName",
                "repoName"
              ],
              "selectedProperties": [
                "inSightsTimeX",
                "inSightsTime",
                "toolName",
                "categoryName"
              ]
            }
          }
        }
      }
    }
  },
  "agentId": "",
  "enableDataValidation": true,
  "enableCommitFileUpdation": true,
  "toolCategory": "SCM",
  "toolsTimeZone": "GMT",
  "insightsTimeZone": "Asia/Kolkata",
  "enableValueArray": false,
  "useResponseTemplate": true,
  "auth": "base64",
  "runSchedule": 30,
  "timeStampField": "commitTime",
  "timeStampFormat": "%Y-%m-%dT%H:%M:%SZ",
  "isEpochTimeFormat": false,
  "almKeyRegEx": "([A-Z]{1}[A-Z0-9]+\\s?-\\s?\\d+)",
  "agentSecretDetails": [
    "accessToken"
  ],
  "startFrom": "2021-06-01 15:46:33",
  "accessToken": "",
  "getRepos": "https://api.github.com/users/<USER_NAME>/repos",
  "commitsBaseEndPoint": "https://api.github.com/repos/<REPO_NAME>/",
  "isDebugAllowed": false,
  "loggingSetting": {
    "logLevel": "INFO",
    "maxBytes": 5000000,
    "backupCount": 10
  },
  "labelName": "GIT",
  "toolName": "GIT",
  "auditing": false,
  "enableProxy": false,
  "proxies": {
    "http": "",
    "https": ""
  }
}