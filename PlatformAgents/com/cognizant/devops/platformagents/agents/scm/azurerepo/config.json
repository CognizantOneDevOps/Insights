{
  "vault": {
    "getFromVault": false,
    "secretEngine":"insights-kv",
    "readToken":"vault_secret_token",
    "vaultUrl":"http://Vault_Host:port/v1/"
  },
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg": "iAgent"
  },
  "subscribe": {
    "config": "SCM.AZUREREPO.config"
  },
  "publish": {
    "data": "SCM.AZUREREPO.DATA",
    "health": "SCM.AZUREREPO.HEALTH"
  },
   "enrichData":{ 
      "isEnrichmentRequired":false,
      "sourceProperty":"message",
      "keyPattern":"-",
      "targetProperty":"key"
   },
  "communication": {
    "type": "REST",
    "sslVerify": true,
    "responseType": "JSON"
  },
  "dynamicTemplate": {
   "branches":{
         "insightsTimeXFieldMapping":{
            "timefield":"consumptionTime",
            "isEpoch":false,
            "timeformat":"%Y-%m-%dT%H:%M:%SZ"
         }
      },
    "timeFieldMapping": {
      "startDate": "%Y-%m-%d"
    },
    "responseTemplate": {
      "commitId": "commitId",
      "comment": "message",
      "author": {
        "name": "authorName",
        "email": "authorEmail",
        "date": "commitTime"
      }
    }
  },
  "agentId": "",
  "enableBranches": true,
  "enableDataValidation": true,
  "enableBrancheDeletion": true,
  "toolCategory": "SCM",
  "toolsTimeZone": "GMT",
  "insightsTimeZone": "UTC",
  "enableValueArray": false,
  "useResponseTemplate": true,
  "auth": "NTLM",
  "runSchedule": 30,
  "timeStampField": "commitTime",
  "timeStampFormat": "%Y-%m-%dT%H:%M:%SZ",
  "isEpochTimeFormat": false,
  "startFrom": "2015-01-01 00:00:00",
  "userid": "",
  "passwd": "",
  "accessToken": "accesstoken",
  "agentSecretDetails": ["userid","passwd"],
  "getRepos": "https://dev.azure.com/<organization>/<project>/_apis/git/repositories",
  "commitsBaseEndPoint": "https://dev.azure.com/<organization>/<project>/_apis/git/repositories/",
  "isDebugAllowed": false,
  "loggingSetting": {
    "logLevel": "INFO",
    "maxBytes": 5000000,
    "backupCount": 1000
  }
}
