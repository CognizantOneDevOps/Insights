{
  "vault": {
    "getFromVault": false,
    "secretEngine": "insights-kv",
    "readToken": "vault_secret_token",
    "vaultUrl": "http://Vault_Host:port/v1/"
  },
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg": "iAgent"
  },
  "subscribe": {
    "config": "CI.AZUREPIPELINE.config"
  },
  "publish": {
    "data": "CI.AZUREPIPELINE.DATA",
    "health": "CI.AZUREPIPELINE.HEALTH"
  },
  "communication": {
    "type": "REST",
    "sslVerify": false,
    "responseType": "JSON"
  },
  "dynamicTemplate": {
    "timeFieldMapping": {
      "startDate": "%Y-%m-%d"
    },
    "responseTemplate": {
      "result": "result",
      "id": "buildId",
      "sourceVersion": "sourceVersion",
      "sourceBranch": "sourceBranch",
      "queueTime": "queuedDate",
      "reason": "manual",
      "requestedBy": {
        "displayName": "ciAuthorName",
        "uniqueName": "ciAuthor"
      },
      "status": "status",
      "repository": {
        "type": "repositoryType",
        "name": "repositoryName"
      },
      "buildNumber": "buildNumber",
      "startTime": "startDate",
      "finishTime": "finishDate",
      "definition": {
        "name": "buildDefinitionName"
      },
      "url": "buildUrl",
      "priority": "priority"
    }
  },
  "agentId": "",
  "toolCategory": "CI",
  "toolsTimeZone": "Asia/Kolkata",
  "insightsTimeZone": "Asia/Kolkata",
  "enableDataValidation": true,
  "isDebugAllowed": false,
  "enableValueArray": false,
  "useResponseTemplate": true,
  "runSchedule": 30,
  "auth": "BASIC",
  "userid": "",
  "passwd": "",
  "baseUrl": "https://dev.azure.com",
  "collectionName": "",
  "timeStampField": "startDate",
  "timeStampFormat": "%Y-%m-%dT%H:%M:%S",
  "isEpochTimeFormat": false,
  "isStartFromDateEnabled": true,
  "startFromDate": "2020-01-01",
  "agentSecretDetails": [
    "userid",
    "passwd"
  ],
  "loggingSetting": {
    "logLevel": "WARN",
    "maxBytes": 5000000,
    "backupCount": 1000
  }
}
