{
  "vault": {
    "getFromVault": false,
    "secretEngine":"insights-kv",
    "readToken":"vault_secret_token",
    "vaultUrl":"http://Vault_Host:port/v1/"
  },
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg": "iAgent"
  },
  "subscribe": {
    "config": "ENVIRONMENT.AWS.config"
  },
  "publish": {
    "data": "ENVIRONMENT.AWS.DATA",
    "health": "ENVIRONMENT.AWS.HEALTH"
  },
  "communication": {
    "type": "REST",
    "sslVerify": true,
    "responseType": "JSON"
  },
  "dynamicTemplate" : {
  		"timeFieldMapping" : {
			"startDate" : "%Y-%m-%d"
  		}
   },	
   "agentId" : "",
  "toolCategory": "ENVIRONMENT",
  "enableDataValidation": true,
  "toolsTimeZone": "GMT",
  "insightsTimeZone": "Asia/Kolkata",
  "useResponseTemplate": false,
  "auth": "base64",
  "runSchedule": 2,
  "timeStampField": "launchTime",
  "timeStampFormat": "epoch",
  "isEpochTimeFormat" : false,
  
  "pattern": "%Y-%m-%dT%H:%M:%S.%fZ",
  "dynamicTemplate": {
    "vpc_id": [
      "vpc-id1",
      "vpc-id2"
    ]
  },
  "access_key_id": "#Access_Key_ID#",
  "secret_access_key": "#secret_key#",
  "isDebugAllowed": false,
  "enableValueArray": false,
  "agentSecretDetails": ["access_key_id","secret_access_key"],
  "loggingSetting": {
    "logLevel": "WARN",
	"maxBytes" : 5000000,
	"backupCount" : 1000
  }
}